#include <iostream>
#include <stack>

using namespace std;

void printStack(stack<int>stack){
    while(!stack.empty()){
        cout<<stack.top()<<endl;
        stack.pop();
    }
}

int main(){
    stack<int>number;

    for(int i = 0; i<5;i++){
    number.push(i);
    }
    printStack(number);
}
